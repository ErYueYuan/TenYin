page {
  background: #F8F9FB;
}

.title {
  font-size: 28rpx;
  color: #333333;
  margin: 58rpx 0 0 32rpx;
}

.pic {
  width: 626rpx;
  height: auto;
  margin: 0 auto;
  padding: 32rpx 30rpx;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  border-bottom: 1rpx solid #EEEEEE;
}

.pic image {
  width: 180rpx;
  height: 100rpx;
  margin-right: 26rpx;
}

.pic image:last-child {
  margin-right: 0;
}

.photos,
.camera {
  width: 634rpx;
  height: auto;
  background: #FFFFFF;
  box-shadow: 0 2rpx 14rpx 0 rgba(62, 47, 152, 0.04);
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 36rpx 26rpx;
  margin: 24rpx auto auto auto;
}

.right image {
  width: 12rpx;
  height: 22rpx;
}

.left {
  display: flex;
  align-items: center;
}

.left image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 26rpx;
}

.text {
  font-size: 30rpx;
  color: #333333;
}

@import "../../compoents/toast_text/toast.wxss";
@import "../../compoents/toast_notice/toast.wxss";